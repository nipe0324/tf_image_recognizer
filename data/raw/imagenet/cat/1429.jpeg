<script type="text/javascript">
	function sliderLazyload(container, pos, width, height, callback) {
		var currentImgContainer = container.find('ul li').eq(pos - 1).find('.sliderLazyLoad');
		currentImgContainer.each(function() {
			var currentSlider = null;
			var containerId = container.attr('id');
			if (containerId == 'photoFrameSlider') {
				currentSlider = window.photoFrameSlider;				
			}

			if( currentSlider != null ){
				//保存 slider之前的auto 状态
				currentSlider.data("befreAutoanimation", currentSlider.getValue('autoanimation'));
				if(currentSlider.data("befreAutoanimation")){ //如果正在auto，则暂停
					currentSlider.stopAuto();
					//console.log(containerId + '.stopAuto();');
				}
			}

			var lazyHtml = $(this).html();
			var itemImg = lazyHtml.match(/<!--(.*)-->/)[1];
			var img = $(itemImg);
			if (width > 0) {
				img.load(function() {
					$(this).imgResizeV4({
						width: width,
						height: height,
						setOnlyOneSide: true,
						completed: function() {
							callback.call(this, containerId);
						}
					});
				});
			}
			$(this).replaceWith(img);
		});
	}
	//slider 中的图片加载完成后触发

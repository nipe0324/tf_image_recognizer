  $('.cateLink').hover(function (event) {
    var lMenu_layer = $(this).addClass('cateLinkOn').children('.lMenu_layer').show();
    lMenu_layer.css('top', 0);
    var headerHeight = Math.max($('#header').outerHeight(true), $('.header_c').outerHeight());
    var windowHeight = $(window).height();
    var maxHeight = windowHeight - headerHeight; //中间区域高度
    var menuHeight = lMenu_layer.outerHeight(); //菜单高度
    var menuInitTop = $(this).offset().top - $(document).scrollTop();    //初始top
    var isOverflow = (menuHeight + menuInitTop - headerHeight) > maxHeight;
    if(isOverflow){ //是否溢出
      if(menuHeight <= maxHeight){ //中间区域是否够大
        var offsetTop = menuInitTop + menuHeight - windowHeight;
        lMenu_layer.css('top', -offsetTop);
      }else{
        //不够高，则设置top 最顶
        lMenu_layer.css('top', -(menuInitTop - headerHeight));
      }
    }else{
      lMenu_layer.css('top', -1);
    }
  }, function (event) {
    $(this).removeClass('cateLinkOn').children('.lMenu_layer').hide();
  });

<script>
function dismiss_bs(){
		$('#browser_suggest').removeClass('active');
		$.cookie('hide_bs',true,{path:'/',expires:7});
	}
$(document).ready(function(){
	var	bscc = 30;
	var isMobile = 'False' == 'True' ? true:false;
	
	function msieversion() {

			var ua = window.navigator.userAgent;
			var msie = ua.indexOf("MSIE ");

			if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))      // If Internet Explorer, return version number
				return (parseInt(ua.substring(msie + 5, ua.indexOf(".", msie))));
			else                 // If another browser, return 0
				return 0;

	   return false;
	}
	function closeBrowserSuggest(){
		if (bscc-- > 0){
			$('#bs_tick').html(bscc);
			setTimeout(closeBrowserSuggest,1000);
		}
		else{
			$('#browser_suggest').removeClass('active');
		}
	}
	function isChrome(){
		return navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
	}
	function getChromeVersion () {     
		var raw = navigator.userAgent.match(/Chrom(e|ium)\/([0-9]+)\./);

		return raw ? parseInt(raw[2], 10) : false;
	}

	if (IsLogin && (!isChrome() || (isChrome() && getChromeVersion() < 48 )) && !isMobile && !$.cookie('hide_bs')   ){
	
		setTimeout(closeBrowserSuggest,1000);
		$('#browser_suggest').addClass('active');
	}


});




</script>
        

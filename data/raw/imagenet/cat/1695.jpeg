		catch(er) {}
		}

	function FormSearchSubmit()
		{
		var i, v, ie

		// Screen Width
		ie = (document.all)? true:false
		if (ie)
			document.formsearchsubmit.screenwidth.value = document.body.offsetWidth
		else
			document.formsearchsubmit.screenwidth.value = window.innerWidth

		// Results Per Page
		i = document.formsearch.pixperpage.selectedIndex
		v = document.formsearch.pixperpage.options[i].value
		document.formsearchsubmit.pixperpage.value = v

		// Keywords
		if (!document.formsearchsubmit.searchtxtkeys.value)
			{
			v = document.formsearch.txtkeys1.value

			if (v.toLowerCase() == "search by keyword(s) or image id") v = ""
			v = Trim(v)
			if (v) document.formsearchsubmit.searchtxtkeys.value = v

			if (document.formsearch.chkwithin.checked)
				document.formsearchsubmit.withinresults.value = "yes"
			}

		// Color, BW
		if (!document.formsearchsubmit.lstformats.value)
			{
			if (!document.formsearch.chkcolor.checked && !document.formsearch.chkbw.checked)
				document.formsearchsubmit.lstformats.value = ""
			else if (document.formsearch.chkcolor.checked && !document.formsearch.chkbw.checked)
				document.formsearchsubmit.lstformats.value = "0"
			else if (!document.formsearch.chkcolor.checked && document.formsearch.chkbw.checked)
				document.formsearchsubmit.lstformats.value = "1"
			else if (document.formsearch.chkcolor.checked && document.formsearch.chkbw.checked)
				document.formsearchsubmit.lstformats.value = ""
			}

		// Horizontal, Vertical, Square
		if (!document.formsearchsubmit.lstorients.value)
			{
			v = ""
			if (document.formsearch.chkhor.checked) v = "1"
			if (document.formsearch.chkver.checked) v = v + "2"
			if (document.formsearch.chksqu.checked) v = v + "3"
			if (v && v != "123")
				document.formsearchsubmit.lstorients.value = v
			else
				document.formsearchsubmit.lstorients.value = "All Orientations"
			}

		// Free for Museum staff
		if (!document.formsearchsubmit.chkmodrel.value)
			{
			if (document.formsearch.chkfree.checked)
				document.formsearchsubmit.chkmodrel.value = "TRUE"
			}

		// Photographs, Illustrations, Portraits, Archive, Fossils, Specimens, SEM
		if (document.formsearch.chkpho.checked) document.formsearchsubmit.chkpho.value = "1"
		if (document.formsearch.chkill.checked) document.formsearchsubmit.chkill.value = "1"
		if (document.formsearch.chkpor.checked) document.formsearchsubmit.chkpor.value = "1"
		if (document.formsearch.chkarc.checked) document.formsearchsubmit.chkarc.value = "1"
		if (document.formsearch.chkfos.checked) document.formsearchsubmit.chkfos.value = "1"
		if (document.formsearch.chkspe.checked) document.formsearchsubmit.chkspe.value = "1"
		if (document.formsearch.chksem.checked) document.formsearchsubmit.chksem.value = "1"

		// Submit Form
		document.formsearchsubmit.submit()
		}

	function WithinResultsOnClick()
		{
		if (document.formsearch.chkwithin.checked)
			{
			if (document.formsearch.txtkeys1.value == "")
				{
				document.formsearch.txtkeys1.value = ""
				document.formsearch.txtkeys1.focus()
				}
			}
		else
			{
			document.formsearch.txtkeys1.value = ""
			}
		}

	function TNZoomOnClick()
		{
		var url

		if (document.formsearch.chktnzoom.checked)
			url = "ajaxtnzoom.asp?tnzoom=yes"
		else
			url = "ajaxtnzoom.asp?tnzoom=no"

		try {HTTP_RequestTNZoom(url)}
		catch(er) {}
		}

	function PixPerPageOnChange()
		{
		var i, v, RequestW, RequestF

		RequestW = ""
		RequestF = ""

		if (RequestW && RequestF)
			{
			i = document.formsearch.pixperpage.selectedIndex
			v = document.formsearch.pixperpage.options[i].value
			location = "http://piclib.nhm.ac.uk/results.asp?W=" + RequestW + "&F=" + RequestF + "&Step=1&pixperpage=" + v
			}
		}

	function SearchPrevOnChange()
		{
		var i = document.formsearch.searchprev.selectedIndex
		var txtkeys1 = document.formsearch.searchprev.options[i].value
		document.formsearch.txtkeys1.value = txtkeys1
		FormSearchSubmit()
		}

	function Trim(sString)
		{
		while (sString.substring(0, 1) == ' ')
			sString = sString.substring(1, sString.length)

		while (sString.substring(sString.length - 1, sString.length) == ' ')
			sString = sString.substring(0, sString.length - 1)

		return sString
		}

</script>



<div id="greyinlinewindow" style="position:absolute; left:0px; top:0px; width:100%; background-color:#000000; z-index:100; display:none;"></div>

<div id="emailinlinewindow" style="position:fixed; top:50px; left:0px; width:100%; height:100%; z-index:200; display:none">
	<div align="center">
		<div align="left" style="position:relative; margin-top:100px; width:300px; border-style:solid; border-color:#000077; border-width:2px; background-color:#ffffff; z-index:2">
			<div style="position:relative; width:100%; height:23px; background-color:#000077">
				<div style="float:left; font-size:16px; color:#ffffff; font-weight:bold; margin-left:2px">Email this page</div>
				<div style="float:right; margin:0 0 0 0"><a href="javascript:CloseInlineWindow('emailinlinewindow')" onmouseover="document.images['divclosecopy'].src = 'images/divclose_on.gif'" onmouseout="document.images['divclosecopy'].src = 'images/divclose.gif'"><img src="images/divclose.gif" name="divclosecopy" width="21" height="21" alt="Close" title="Close" border="0" /></a></div>
				<div style="clear:both"></div>
			</div>
			<div style="position:relative; width:auto; margin:8px 10px 10px 10px; font-size:12px;">
				<form name="formemailthispage" method="get" style="border:0; padding:0" action="javascript:EmailThisPageSubmit()">
					<table border="0" cellspacing="0" cellpadding="0">
						<tr>
							<td align="left"><strong>Email to:</strong></td>
							<td align="left"><input type="text" name="email" style="width:200px" /></td>
						</tr>
						<tr>
							<td align="left" valign="top">Message:</td>
							<td align="left"><textarea name="message" rows="5" style="width:200px" onkeyup="EmailThisPageMessageOnKeyUp()"></textarea></td>
						</tr>
						<tr>
							<td></td>
							<td><input type="submit" name="submit" value="Send" style="cursor:pointer" /></td>
						</tr>
					</table>
				</form>
			</div>
		</div>
	</div>
</div>
<div style="clear:both"></div>

<script type="text/javascript">

	var http_requestInlineWindow
	var InlineWindowAction	



		function OpenInlineWindow(dialogwindow)
			{
			alert("You must be registered and signed in to email this page.")
			}



	function EmailThisPageMessageOnKeyUp()
		{
		s = document.formemailthispage.message.value

		if (s.length > 1700)
			document.formemailthispage.message.value = s.substring(0, 1800)
		}

	function EmailThisPageSubmit()
		{
		var email
		email = Trim(document.formemailthispage.email.value)
		message = Trim(document.formemailthispage.message.value)
		
		if (email)
			{
			InlineWindowAction = "emailthispage"	
			url = "ajaxinlinewindow.asp?page=404.asp&emailthispage=" + escape(email) + "&message=" + escape(message)
			HTTP_RequestInlineWindow(url)
			}
		else
			{
			alert("Please include the email address to send to.")
			}
		}
	
	function HTTP_RequestInlineWindow(url) 
		{
		http_requestInlineWindow = false

		if (window.XMLHttpRequest) 
			{
			http_requestInlineWindow = new XMLHttpRequest()
			if (http_requestInlineWindow.overrideMimeType) http_requestInlineWindow.overrideMimeType('text/xml')
			} 
		else if (window.ActiveXObject) 
			{
			try 
				{http_requestInlineWindow = new ActiveXObject("Msxml2.XMLHTTP")} 
			catch(er) 
				{
				try {http_requestInlineWindow = new ActiveXObject("Microsoft.XMLHTTP")} 
				catch(er2) {}
				}
			}

		if (http_requestInlineWindow)	
			{
			http_requestInlineWindow.open('GET', url, true)
			http_requestInlineWindow.onreadystatechange = HTTP_WriteInlineWindow
			http_requestInlineWindow.send(null)
			}
		}
		
	function HTTP_WriteInlineWindow()
		{
		var info
		 
		if (http_requestInlineWindow.readyState == 4 && http_requestInlineWindow.status == 200) 
			{
			info = http_requestInlineWindow.responseText
			
			if (InlineWindowAction == "emailthispage")
				{
				alert(info)
				CloseInlineWindow('emailinlinewindow')
				}
			}
		}
		
</script>



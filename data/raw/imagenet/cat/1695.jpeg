<div id="resultsTnDiv" style="position:absolute; left:0px; top:0px; width:0px; height:0px; background-color:#ffffff; layer-background-color:#ffffff; z-index:100; visibility:hidden"></div>

<script type="text/javascript">

	// Variables divsizex and divsizey represent the div layer's width and height.
	// The layer contains the image with padding of 30px on the top, left, and right, and 40px on the bottom.
	// The divsizex and divsizey values are returned from ajaxtnzoom.asp as being "~~~~~" delimited.
	// They are used in SetDivXY() to correctly position the layer.
	var divsizex = 500
	var divsizey = 500

	var docx = 0  // The current mouse position where 0,0 is the top left corner of the document.
	var docy = 0

	var divx, divy   // The coordinates to move the div layer to, where 0,0 is the top left corner of the document.

	var screenx = 0  // The current position of the mouse where 0,0 is the top left corner of the browser window.
	var screeny = 0

	var scrollx = 0  // This is the amount that the document is currently scrolled.
	var scrolly = 0

	var clientwidth = 0  // This is the size of the browser window. For Opera, it's an error because it's the size of the document, but we don't care about Opera for now.
	var clientheight = 0

	var ImageTop = false // Set to true if on the current mouseover, the image was placed near the top of the screen.

	var imageid  // Image ID.
	var imagex   // Position of image in the search result starting from 1.

	var resultsTnDivVisible = false // True if the layer is currently visible.

	var timeoutID
	var ns4 = (document.layers)? true:false
	var ie4 = (document.all)? true:false

	var MouseOverFlag = false // MouseOverFlag = True if an image is moused over. Sometimes the thumbnail zoom layer will remain visible after mouse out. This variable is used to trap the event and make the layer hidden.

	function resultsTnMouseOver(paramimageid, paramimagex)
		{
		try
			{
			if (document.formsearch.chktnzoom.checked)
				{
				imageid = paramimageid
				imagex = paramimagex
				timeoutID = setTimeout('resultsTnMouseOver2()', 500)
				}
			}
		catch(er) {}
		}

	function resultsTnMouseOver2()
		{
		MouseOverFlag = true
		HTTP_RequestTNZoom("http://piclib.nhm.ac.uk/ajaxtnzoom.asp?imageid=" + imageid)
		}

	function resultsTnMouseOut(Imageid, Imagex)
		{
		clearTimeout(timeoutID)
		resultsTnDivVisible = false
		HideLayer('resultsTnDiv')
		ImageTop = false
		MouseOverFlag = false
		}

	if (!ie4) document.captureEvents(Event.MOUSEMOVE)
	document.onmousemove = getXYs

	function getXYs(e)
		{
		// This gets called whenever the mouse moves.
		// It populates values and moves the layer if it's visible.

		// <!DOCTYPE html PUBLIC ...> must be included. If not, then:
		// - docx = event.clientX + document.body.scrollLeft
		// - The values for scrollx and scrolly would not be obtainable.

		if (ie4)
			{
			screenx = event.clientX + document.body.scrollLeft
			screeny = event.clientY + document.body.scrollTop
			docx = screenx + scrollx
			docy = screeny + scrolly

			if (document.documentElement)
				{
				scrollx = document.documentElement.scrollLeft
				scrolly = document.documentElement.scrollTop
				clientwidth = document.documentElement.clientWidth
				clientheight = document.documentElement.clientHeight
				}
			else if (document.body)
				{
				scrollx = document.body.scrollLeft
				scrolly = document.body.scrollTop
				clientwidth = document.body.clientWidth
				clientheight = document.body.clientHeight
				}
			}
		else
			{
			docx = e.pageX
			docy = e.pageY
			screenx = docx - scrollx
			screeny = docy - scrolly
			scrollx = window.pageXOffset
			scrolly = window.pageYOffset
			clientheight = window.innerHeight
			clientwidth = window.innerWidth
			}

		if (docx < 0) docx = 0
		if (docy < 0) docy = 0

		if (resultsTnDivVisible)
			{
			SetDivXY()
			MoveLayer('resultsTnDiv', divx, divy)
			ShowLayer('resultsTnDiv')
			}
		else
			{
			HideLayer('resultsTnDiv')
			}

		var eresultsTnDiv = new getObj('resultsTnDiv')
		if (MouseOverFlag == false && eresultsTnDiv.style.visibility != "hidden")
			resultsTnMouseOut(imageid, imagex)
		}

	function SetDivXY()
		{
		// This function sets the variables divx and divy to be the new values of the layer's position.
		// It's called just before moving the layer.

		var divxoffset // Distance to move the layer horizontally if the layer is vertically centered.

		// Place layer below the cursor and to the right. This is the default position.
		divx = docx + 10
		divy = docy + 10
		divxoffset = 10

		if (screenx + divsizex + 50 > clientwidth)
			// The cursor is too close to the right, so place layer to the left.
			{
			divx = divx - divsizex - 20
			divxoffset = -10
			}

		if (screeny + divsizey + 20 > clientheight)
			{
			// The cursor is too close to the bottom of the page.
			if (screeny < divsizey || ImageTop == true)
				{
				// The cursor is also too close to the top, so place the layer near the top.
				divy = scrolly + 5  // Place the layer near the top.
				divx += divxoffset  // Place the layer further from the cursor. This makes it less likely for a fast moving cursor to run over the layer, thus calling resultsTnMouseOut().
				ImageTop = true     // This is so that the image won't jump back to being above the cursor.
				}
			else
				// Place layer above the cursor.
				divy = divy - divsizey - 20
			}
		}


	// Below are generic layer functions.

	function ResizeLayer(ID, w, h)
		{
		var e = new getObj(ID)
		e.style.width = w + "px"
		e.style.height = h + "px"
		}

	function MoveLayer(ID, x, y)
		{
		var e = new getObj(ID)
		e.style.left = x + "px"
		e.style.top = y + "px"
		}

	function ShowLayer(ID)
		{
		var e

		if (document.getElementById)
			{
			e = document.getElementById(ID)
			e.style.visibility = "visible"
			}
		else if (document.all)
			{
			var e = document.all["ID"]
			e.style.visibility = "visible"
			}
		else if (document.layers)
			{
			e = document.layers["ID"]
			e.style.visibility = "show"
			}
		}

	function HideLayer(ID)
		{
		var e
		if (document.getElementById)
			{
			e = document.getElementById(ID)
			e.style.visibility = "hidden"
			}
		else if (document.all)
			{
			var e = document.all["ID"]
			e.style.visibility = "hidden"
			}
		else if (document.layers)
			{
			e = document.layers["ID"]
			e.style.visibility = "hide"
			}
		}

	function LeftChr(s, d)
		{
		var i, a

		i = s.indexOf(d)

		if (i > -1)
			a = s.substr(0, i)
		else
			a = s

		return(a)
		}

	function RightChr(s, d)
		{
		var i, a

		i = s.indexOf(d)

		if (i > -1)
			a = s.substring(i + d.length)
		else
			a = s

		return(a)
		}

	function getObj(name)
		{
		if (document.getElementById)
			{
			this.obj = document.getElementById(name)
			this.style = document.getElementById(name).style
			}
		else if (document.all)
			{
			this.obj = document.all[name]
			this.style = document.all[name].style
			}
		else if (document.layers)
			{
			this.obj = document.layers[name]
			this.style = document.layers[name]
			}
		}


	// Below are functions used for Ajax.

	var http_requestTNZoom

	function HTTP_RequestTNZoom(url)
		{
		http_requestTNZoom = false

		if (window.XMLHttpRequest)
			{
			http_requestTNZoom = new XMLHttpRequest()
			if (http_requestTNZoom.overrideMimeType) http_requestTNZoom.overrideMimeType('text/xml')
			}
		else if (window.ActiveXObject)
			{
			try
				{http_requestTNZoom = new ActiveXObject("Msxml2.XMLHTTP")}
			catch(er)
				{
				try {http_requestTNZoom = new ActiveXObject("Microsoft.XMLHTTP")}
				catch(er2) {}
				}
			}

		if (http_requestTNZoom)
			{
		//	document.getElementById('resultsTnDiv').innerHTML = "<center><br /><br /><br /><br />Preparing Image for Viewing...</center>"
			document.getElementById('resultsTnDiv').innerHTML = ""
			http_requestTNZoom.open('GET', url, true)
			http_requestTNZoom.onreadystatechange = HTTP_WriteTNZoom
			http_requestTNZoom.send(null)
			}
		}

	function HTTP_WriteTNZoom()
		{
		var info, x
	   if (http_requestTNZoom.readyState == 4 && http_requestTNZoom.status == 200)
			{
			info = http_requestTNZoom.responseText

			if (info.indexOf('FAIL1234567890') == -1)
				{
				x = RightChr(info, "~~~~~")
				info = LeftChr(info, "~~~~~")
				divsizex = LeftChr(x, "~~~~~")
				divsizey = RightChr(x, "~~~~~")

				try
					{divsizex *= 1; divsizey *= 1;}
				catch(er)
					{divsizex = 500; divsizey = 500;}

				ResizeLayer('resultsTnDiv', divsizex, divsizey)

				document.getElementById('resultsTnDiv').innerHTML = info
				resultsTnDivVisible = true
				SetDivXY()
				MoveLayer('resultsTnDiv', divx, divy)
				ShowLayer('resultsTnDiv')
				}
			}
		}

	function ZoomPrevOnClick()
		{
		// Calls tnzoomhttp.asp to set the state of Session("asp_zoomprev") to either "yes" or "no".
		// If Session("asp_tnzoom") = "yes", TN Zoom will be enabled.

		try
			{
			if (document.formsearch.zoomprev.checked)
				url = "http://piclib.nhm.ac.uk/ajaxtnzoom.asp?tnzoom=yes"
			else
				url = "http://piclib.nhm.ac.uk/ajaxtnzoom.asp?tnzoom=no"

			http_request = false

			if (window.XMLHttpRequest)
				{
				http_request = new XMLHttpRequest()
				if (http_request.overrideMimeType) http_request.overrideMimeType('text/xml')
				}
			else if (window.ActiveXObject)
				{
				try
					{http_request = new ActiveXObject("Msxml2.XMLHTTP")}
				catch(er)
					{
					try {http_request = new ActiveXObject("Microsoft.XMLHTTP")}
					catch(er2) {}
					}
				}

			if (http_request)
				{
				http_request.open('GET', url, true)
				http_request.send(null)
				}
			}
		catch(er) {}
		}

</script>



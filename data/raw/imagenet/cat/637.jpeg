//<![CDATA[ 
$(document).ready(function(){
	$("#searchBN").button();
	$("#reportURLbn").button();
	$("#reportTEXTbn").button();
	$("#reportTEXTcancelBn").button();
	$("#feedback_introBN").button();
	$("#searchBN").click(function() {
		$("#mainSearchForm").submit()
	});
	$("#reportURLbn").click(function() {
		$("#pageSource").val("4O4")
		$("#f404report").submit()
	});
	$("#reportTEXTbn").click(function() {
		$("#pageSource").val("4O4")
		$("#f404report").submit()
	});
	$("#reportTEXTcancelBn").click(function() {
	  $("#feedback_404").slideUp(function() {
	    	$( "#feedback_introBN" ).button( "option", "disabled", false );
	  })
	});
	
	$("#feedback_introBN").click(function() {
		$( "#feedback_introBN" ).button( "option", "disabled", true );
		$("#feedback_404").fadeIn()
	});
	function arrayOccurrence(arrayA){
		occCount=0;
		for(j=0;j<arrayA.length;j++){
			perCount=0
			testL=arrayA[j].value
			//for(i=0;i<arrayA.length;i++){
				//if(arrayA[i].value==testL){
					//remove the desc str by increasing the perCount for the conditional statement below
					//perCount++
				//}
			//}	
			if(perCount==1 && arrayA[j].value.length>5){
				arrayA[j].desc=""; arrayA[j].sub=""
			}else{
				arrayA[j].sub="&lt; "+arrayA[j].sub
			}
			//remove JS links
			if(arrayA[j].url.indexOf("javascript:")!=-1){
				delete arrayA[j].value
			}
		}
		
	
	    return arrayA;
	}
	

		$.ajax({
		    url: "/404/sitemapxcri/compositeSitemapXCRI_with-coreData-and-amps-replaced.xml",
			dataType: "xml",
			success: function( xmlResponse ) {
				var data = $( "li", xmlResponse ).map(function() {
					return {
						value: $( "a", this ).text(),
						url: $( "a", this ).attr("href"),
						desc: $( "a", this ).attr("href").replace("/home/","").replace("studyatlincoln/",""),
						sub: $( this ).attr("subSection")
					};
				}).get();
				
				data=arrayOccurrence(data)
				data.sort(function(a,b){
				    if(a.value<b.value) return -1;
				    if(a.value>b.value) return 1;
				    return 0;
				})
				
				suggestions=wereYouLookingFor(data)
				
				if(typeof suggestions!="undefined"){
					if(suggestions.length>0){
						newSuggestionsStr=""
						for(p=0;p<suggestions.length;p++){
							newSuggestionsStr=newSuggestionsStr+"<a href='"+suggestions[p].url+"'>"+suggestions[p].value+"</a> "+suggestions[p].sub+"<br />"
						}
						$("#suggestions_urlBased").html(newSuggestionsStr)
						$("#suggestions_urlBasedOuter").slideDown()
					}
				}

				$( "#searchQuery" ).autocomplete({
					source: data,
					minLength: 3,
					select: function(event, ui) {
						document.location.href=ui.item.url
					}
				})
				.data( "autocomplete" )._renderItem = function( ul, item ) {
					return $( "<li></li>" )
					.data( "item.autocomplete", item )
					.append( "<a>" + item.value + " <span class='autoCompleteURLstr'>" + item.sub +"</span></a>" )
					.appendTo( ul );
				};
			}
		});
		//limit those results!
		$.ui.autocomplete.prototype._renderMenu = function( ul, items ) {
			   var self = this;
			   $.each( items, function( index, item ) {
			      if (index < 10) // here we define how many results to show
			         {self._renderItem( ul, item );}
			      });
			}
		//--------------------//
		
		//Define 'Were you looking for?'
		function wereYouLookingFor(dataArray){
			var suggestions=[]
			if(failedURL){
				failedURL=failedURL.replace("home/","")
				failedURL=failedURL.replace("studyatlincoln/","")
				failedURL=failedURL.replace("postgraduateprogrammes/","")
				failedURL=failedURL.replace("undergraduatecourses/","")
				failedURL=failedURL.replace("course/","")
				failedURLs=failedURL.split("/")
				for(t=failedURLs.length; t>0; t--){
					if(typeof failedURLs[t]!="undefined" && failedURLs[t].indexOf(".")==-1 && failedURLs[t]!=""){
						//search json
						for(u=0;u<dataArray.length;u++){
							jsonStr=dataArray[u].url
							if(jsonStr.indexOf("/"+failedURLs[t]+"/")!=-1){
								//check that this url hasn't already been added
								var foundDup=0
								for(r=0; r<suggestions.length; r++){
									lookingFor=dataArray[u].value
									if(suggestions[r].value.indexOf(lookingFor)!=-1){
										foundDup++
									}
								}
								if(foundDup==0){
									
									suggestions.push({value:dataArray[u].value, url:dataArray[u].url, desc:dataArray[u].desc, sub:dataArray[u].sub})
									if(suggestions.length==4){return suggestions}
								}
							}
						}
					}
				}
			}return suggestions
		}
})

function respondToUser(respondOK){
	if(respondOK==1){
		$("#feedback_404").html('<div class="ui-state-highlight ui-corner-all" id="responseDiv" style="margin: 20px 0 20px 0; padding: 5px; background: #ffef68; width: 800px; text-align:center"><p><strong>Thank-you.</strong> We have logged your broken link report and will investigate it as soon as possible.</p></div>')
		$('#responseDiv').animate({backgroundColor:"#fcfaf1"},1000)
	}else{
		$("#feedback_404").html('<div class="ui-state-error ui-corner-all" id="responseDiv" style="margin: 20px 0 20px 0; padding: 5px; background: #ff6868; width: 550px; text-align:center"><p><strong>Sorry.</strong> We were unable to complete your request at this time.</p></div>')
		$('#responseDiv').animate({backgroundColor:"#fef8f6"},1000)
	}
}

var maxEnquiry=1000

function textCounter(field, maxlimit) {
		if (field.value.length > maxlimit){
			field.value = field.value.substring(0, maxlimit);
			$(field).css("background","url(images/phpThumb_generated_thumbnail.png)")
			$(".tip_404 STRONG").css("color","#000")
		}else {
			if (field.value.length < maxlimit){
				$(field).css("background","")
				$(".tip_404 STRONG").css("color","")
			}
		}
	}
//]]> 
